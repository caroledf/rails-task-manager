<div class="checkbox">
<% @tasks.each do |task| %>
<%#= link_to tasks_path %>
<%#= link_to "Studying", tasks_path %>
  <h2><%= link_to task.title, task_path(task) %></h2>
  <% if task.completed %>
    <input type="checkbox" checked>
  <% else %>
    <input type="checkbox">
  <% end %>
    <!-- <input type="checkbox" <%#= "checked" if task.completed  %>> -->
<% end %>
</div>
